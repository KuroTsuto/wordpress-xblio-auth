{
    "name": "bosconian-dynamics/xblio-auth",
    "description": "A WordPress plugin enabling Xbox Live authentication and API requests by way of the https://xbl.io service.",
    "type": "wordpress-plugin",
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Adam Bosco",
            "email": "github@adambos.co"
        }
    ],
    "minimum-stability": "beta",
    "autoload": {
        "classmap": ["build.php"],
        "psr-4": {
            "BosconianDynamics\\XblioAuth\\": "inc/"
        }
    },
    "scripts": {
    },
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}": ["type:wordpress-plugin"]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/reduxframework/redux-framework-4"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "composer/installers": "^1.9",
        "php-di/php-di": "^6.2",
        "reduxframework/redux-framework-4": "dev-master"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "wp-coding-standards/wpcs": "^2.3"
    }
}
