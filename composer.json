{
    "name": "bosconian-dynamics/xblio-auth",
    "description": "A WordPress plugin enabling Xbox Live authentication and API requests by way of the https://xbl.io service.",
    "type": "wordpress-plugin",
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Adam Bosco",
            "email": "github@adambos.co"
        }
    ],
    "minimum-stability": "beta",
    "autoload": {
        "classmap": ["build.php"],
        "psr-4": {
            "BosconianDynamics\\XblioAuth\\": "inc/",
            "BosconianDynamics\\XblioAuth\\Dependencies\\": "build/vendor/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "\"vendor/bin/mozart\" compose",
            "composer dump-autoload",
            "BosconianDynamics\\XblioAuth\\Build::copy_autoloaders"
        ],
        "post-update-cmd": [
            "\"vendor/bin/mozart\" compose",
            "composer dump-autoload",
            "BosconianDynamics\\XblioAuth\\Build::copy_autoloaders"
        ]
    },
    "extra": {
        "installer-paths": {
            "vendor/{$vendor}/{$name}": ["type:wordpress-plugin"]
        },
        "mozart": {
            "dep_namespace": "BosconianDynamics\\XblioAuth\\Dependencies\\",
            "dep_directory": "/build/vendor/",
            "classmap_directory": "/build/classes/",
            "classmap_prefix": "BDXA_",
            "delete_vendor_directories": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/reduxframework/redux-framework-4"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "composer/installers": "^1.9",
        "php-di/php-di": "^6.2",
        "reduxframework/redux-framework-4": "dev-master"
    },
    "require-dev": {
        "coenjacobs/mozart": "^0.6.0@beta"
    }
}
